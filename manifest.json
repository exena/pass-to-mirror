{
  "manifest_version": 2,
 
  "name": "PassToMirror",
  "description": "click button to go to mirror site",
  "version": "1.0",
 
  "browser_action": {
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "downloads",
    "background",
    "tabs",
    "storage"
  ]
}